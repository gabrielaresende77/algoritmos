algoritmo "14"
// Autora: Gabriela
// 05/12/2024
var
   km, dia, tot: real
inicio
      Escreva ("Qual a quantidade de Km percorridos pelo carro alugado? ")
      Leia (km)
      Escreva ("Qual a quantidade de dias pelos quais ele foi alugado? ")
      Leia (dia)
      tot <- 90*dia + 0.20*km
      Escreva ("O preço total a pagar é R$", tot, ".")
fimalgoritmo