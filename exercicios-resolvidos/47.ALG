algoritmo "47"
// Autora: Gabriela
// 10/12/2024
var
   cont, S: inteiro
inicio
      cont <- 500
      S <- 0
      Enquanto (cont >= 0) faca
               Escreva (cont)
               S <- S + cont
               cont <- cont - 50
      FimEnquanto
      Escreval (" A soma da expressão é: ", S)
fimalgoritmo