algoritmo "46"
// Autora: Gabriela
// 10/12/2024
var
   cont, S: inteiro
inicio
      cont <- 6
      S <- 0
      Enquanto (cont <= 100) faca
               Escreva (cont)
               S <- S + cont
               cont <- cont + 2
      FimEnquanto
      Escreval (" A soma entre esses números é ", S, ".")
fimalgoritmo