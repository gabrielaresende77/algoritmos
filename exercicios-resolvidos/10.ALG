algoritmo "10"
// Autora: Gabriela
// 05/12/2024
var
   L, A, area, tinta: Real
inicio
      Escreva ("Qual a largura da parede (m)? ")
      Leia (L)
      Escreva ("Qual a altura da parede (m)? ")
      Leia (A)
      area <- L*A
      tinta <- area/2
      Escreval ("A área a ser pintada é ", area, "m e a quantidade de tinta necessária para o serviço é ", tinta, "L.")
fimalgoritmo